#!/usr/bin/env python
import sys
from fontTools.ttLib import TTFont
from fontTools.unicode import Unicode

with TTFont(sys.argv[1]) as ttf:
    tables = {}
    for x in ttf["cmap"].tables:
        tables.update(x.cmap)
    for y in tables:
        print("{:04x} {}".format(y,
              tables[y] if not tables[y].startswith("uni")
                        else Unicode[y]))
